<Page xmlns="http://www.nativescript.org/tns.xsd" navigatedTo="onNavigatedTo">
  <Page.actionBar>
    <ActionBar class="action-bar" title="Video Processing">
      <NavigationButton android.systemIcon="ic_menu_back" tap="goBack"/>
    </ActionBar>
  </Page.actionBar>
  <StackLayout>
    <WebView loaded="webViewLoaded" visibility="{{ loaded ? 'visible' : 'collapsed' }}" height="600" id="webView" />
    <Label id="className"></Label>
    <Label id="probability"></Label>
  </StackLayout>
</Page>